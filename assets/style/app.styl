// Import Vuetify styling
@require '~vuetify/src/stylus/main.styl'

.page
  @extend .fade-transition
a{
        color: rgba(0,0,0,0.87)!important;
        transition: 0.3s;
    }
    a:link{
        text-decoration: none;
        color: #000!important;
    }
    a:hover{
        color: rgba(0,0,0,0.50)!important;
        transition: 0.3s;
    }
